    BLUE,
};

add :: inline (a: $T, b: T) -> T {
    return a + b;
}

pub MAIN :: 1;

main :: () {
    f :: 4;
    s :: 4;

    println("{d}", .{add(f,s)});

    for i in 0..9 {
        println("{d}", i);
    }
}

